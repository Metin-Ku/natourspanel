{
  "name": "natours",
  "version": "1.0.0",
  "description": "node, express and mongoDB",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "SET NODE_ENV=production && nodemon server.js",
    "start:prod": "SET NODE_ENV=production && nodemon server.js",
    "build": "npm install",
    "debug": "ndb server.js",
    "watch:all": "npm-run-all --parallel watch:js watch:admin-js compile:sass",
    "watch:js": "parcel watch ./public/Front/js/index.js --out-dir ./public/Front/js --out-file bundle.js --public-url Front/js",
    "watch:admin-js": "parcel watch ./public/Admin/js/index.js --out-dir ./public/Admin/js --out-file bundle.js --public-url /Admin/js",
    "compile:sass": "sass ./public/Admin/sass/main.scss ./public/Admin/css/style.css -w && sass ./public/Front/sass/main.scss ./public/Front/css/style.css -w"
  },
  "author": "Metin",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "alert": "^5.1.4",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "ckeditor5": "^39.0.2",
    "cookie-parser": "^1.4.6",
    "datatables.net-buttons-dt": "^2.4.2",
    "datatables.net-dt": "^1.13.6",
    "datatables.net-responsive-dt": "^2.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.9.0",
    "helmet": "^3.16.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "i18next": "^23.7.7",
    "i18next-fs-backend": "^2.3.0",
    "i18next-http-middleware": "^3.5.0",
    "jquery": "^3.7.1",
    "jquery-validation": "^1.19.5",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^5.13.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nouislider": "^15.7.1",
    "parsleyjs": "^2.9.2",
    "photoswipe": "^5.4.1",
    "pug": "^3.0.2",
    "sharp": "^0.31.0",
    "slugify": "^1.6.6",
    "stripe": "^13.3.0",
    "sweetalert2": "^11.7.27",
    "swiper": "^11.0.4",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.0.1",
    "sass": "^1.66.1",
    "stylelint": "^15.10.2"
  },
  "engines": {
    "node": "^10"
  }
}
